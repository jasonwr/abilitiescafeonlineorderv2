
<div class="container">
    <form id="login" method="post" action="/login">
        <h1>Login</h1>
        <br>
        <p>
            <label>Username:</label>
            <input type="text" name="username">
        </p>
        <p>
            <label>Password:</label>
            <input type="text" name="password">
        </p>
        <p>
            <input type="submit" value="Login">
        </p>
        <h2><%= text %></h2>
    </form>
    <a href="/">Back</a>
</div>



